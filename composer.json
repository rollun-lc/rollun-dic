{
  "name": "rollun-com/rollun-skeleton",
  "description": "Skeleton for app or lib",
  "minimum-stability": "stable",
  "license": "proprietary",
  "type": "project",
  "authors": [
    {
      "name": "avz-cmf",
      "email": "email@example.com"
    },
    {
      "name": "victorynox",
      "email": "it.proffesor02@gmail.com"
    }
  ],

  "require": {
    "php": "^5.6 || ^7.0",
    "roave/security-advisories": "dev-master",
    "zendframework/zend-expressive": "^1.0",
    "zendframework/zend-expressive-helpers": "^2.0",
    "zendframework/zend-expressive-fastroute": "^1.0",
    "zendframework/zend-servicemanager": "^3.0",
    "zendframework/zend-stdlib": "^3",
    "zendframework/zend-db": "2.8.2",
    "zendframework/zend-expressive-twigrenderer": "^1.0",
    "psr/log": "^1.0",
    "rollun-com/rollun-installer": "^2.0",
    "rollun-com/rollun-actionrender": "^4.0",
    "rollun-com/rollun-dic": "^1.0",
    "rollun-com/rollun-datastore": "^3.0",
    "rollun-com/rollun-callback": "^3.0",
    "rollun-com/rollun-permission": "^1.0",
    "rollun-com/rollun-api": "^2.0",
    "zendframework/zend-http": "^2.5"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.0",
    "squizlabs/php_codesniffer": "^2.3",
    "filp/whoops": "^1.1 || ^2.0"
  },
  "autoload": {
    "psr-4": {
      "rollun\\skeleton\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "rollun\\test\\skeleton\\": "tests/src"
    }
  },
  "scripts": {
    "serve": "php -S 127.0.0.1:8080 -t public public/index.php",
    "lib": "rollun\\installer\\Command::command"
  }
}
