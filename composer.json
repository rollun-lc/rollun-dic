{
  "name": "rollun-com/rollun-skeleton",
  "description": "Skeleton for app or lib",
  "minimum-stability": "stable",
  "license": "proprietary",
  "authors": [
    {
      "name": "avz-cmf",
      "email": "email@example.com"
    },
    {
      "name": "victorynox",
      "email": "it.proffesor02@gmail.com"
    }
  ],
  "require": {
    "php": "^5.6 || ^7.0",
    "zendframework/zend-servicemanager": "^3.0",
    "zendframework/zend-stratigility": "^1.1",
    "psr/http-message": "^1.0",
    "zendframework/zend-diactoros": "^1.1",
    "zendframework/zend-config": "^2.6",
    "zendframework/zend-stdlib": "^3",
    "rollun-com/rollun-installer": "dev-master"
  },
  "require-dev": {
    "phpunit/phpunit": "^4.0"
  },
  "autoload": {
    "psr-4": {
      "rollun\\skeleton\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "rollun\\test\\skeleton\\": "tests/src"
    }
  },
  "scripts": {
    "lib-install": "rollun\\installer\\Command::install",
    "lib-uninstall": "rollun\\installer\\Command::uninstall",
    "lib-reinstall": "rollun\\installer\\Command::reinstall"
  }
}
